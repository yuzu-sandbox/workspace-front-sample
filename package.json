{
  "name": "workspace-front-sample",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "s-build": "yarn workspace server run build",
    "s-build:dev": "yarn workspace server run build:dev",
    "c-build": "yarn workspace client run build",
    "c-build:dev": "yarn workspace client run build:dev",
    "build": "run-s c-build s-build",
    "build:dev": "run-p c-build:dev s-build:dev"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
